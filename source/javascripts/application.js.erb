// More info https://github.com/hakimel/reveal.js#configuration
Reveal.initialize({
  history: true,

  // More info https://github.com/hakimel/reveal.js#dependencies
  dependencies: [
    { src: '<%= config[:revealjs_path] %>/plugin/markdown/marked.js' },
    { src: '<%= config[:revealjs_path] %>/plugin/markdown/markdown.js' },
